{
  "version": "1.0",
  "route": {
    "path": "/examples/:slug",
    "layout": "main",
    "meta": {
      "title": "Example - Constela",
      "description": "Interactive example showcasing Constela features.",
      "og:title": "Example - Constela",
      "og:description": "Example - Constela",
      "og:image": "https://constela.dev/og-image.png",
      "og:url": "https://constela.dev/examples",
      "og:type": "website",
      "twitter:card": "summary_large_image",
      "twitter:image": "https://constela.dev/og-image.png"
    }
  },
  "imports": {
    "examples": "../../data/examples.json"
  },
  "getStaticPaths": {
    "source": { "expr": "import", "name": "examples", "path": "examples" },
    "params": {
      "slug": { "expr": "var", "name": "item", "path": "slug" }
    }
  },
  "view": {
    "kind": "element",
    "tag": "article",
    "props": {
      "class": { "expr": "lit", "value": "mx-auto max-w-4xl px-6 py-12 space-y-8" }
    },
    "children": [
      {
        "kind": "element",
        "tag": "header",
        "children": [
          {
            "kind": "element",
            "tag": "a",
            "props": {
              "href": { "expr": "lit", "value": "/examples" },
              "class": { "expr": "lit", "value": "mb-4 inline-flex items-center gap-1 text-sm text-muted-foreground transition-colors hover:text-foreground" }
            },
            "children": [
              {
                "kind": "element",
                "tag": "svg",
                "props": {
                  "xmlns": { "expr": "lit", "value": "http://www.w3.org/2000/svg" },
                  "viewBox": { "expr": "lit", "value": "0 0 24 24" },
                  "fill": { "expr": "lit", "value": "none" },
                  "stroke": { "expr": "lit", "value": "currentColor" },
                  "stroke-width": { "expr": "lit", "value": "2" },
                  "stroke-linecap": { "expr": "lit", "value": "round" },
                  "stroke-linejoin": { "expr": "lit", "value": "round" },
                  "class": { "expr": "lit", "value": "h-4 w-4" }
                },
                "children": [
                  { "kind": "element", "tag": "path", "props": { "d": { "expr": "lit", "value": "M19 12H5M12 19l-7-7 7-7" } } }
                ]
              },
              { "kind": "text", "value": { "expr": "lit", "value": "Back to Examples" } }
            ]
          },
          {
            "kind": "element",
            "tag": "h1",
            "props": {
              "class": { "expr": "lit", "value": "text-3xl font-bold text-foreground" }
            },
            "children": [
              { "kind": "text", "value": { "expr": "get", "base": { "expr": "index", "base": { "expr": "import", "name": "examples", "path": "items" }, "key": { "expr": "route", "name": "slug" } }, "path": "title" } }
            ]
          },
          {
            "kind": "element",
            "tag": "p",
            "props": {
              "class": { "expr": "lit", "value": "mt-2 text-lg text-muted-foreground" }
            },
            "children": [
              { "kind": "text", "value": { "expr": "get", "base": { "expr": "index", "base": { "expr": "import", "name": "examples", "path": "items" }, "key": { "expr": "route", "name": "slug" } }, "path": "description" } }
            ]
          }
        ]
      },
      {
        "kind": "element",
        "tag": "section",
        "children": [
          {
            "kind": "element",
            "tag": "a",
            "props": {
              "href": {
                "expr": "bin",
                "op": "+",
                "left": { "expr": "lit", "value": "/playground?example=" },
                "right": { "expr": "route", "name": "slug" }
              },
              "class": { "expr": "lit", "value": "inline-flex items-center gap-2 rounded-lg bg-primary px-4 py-2 text-sm font-medium text-primary-foreground transition-colors hover:bg-primary/90" }
            },
            "children": [
              {
                "kind": "element",
                "tag": "svg",
                "props": {
                  "xmlns": { "expr": "lit", "value": "http://www.w3.org/2000/svg" },
                  "viewBox": { "expr": "lit", "value": "0 0 24 24" },
                  "fill": { "expr": "lit", "value": "none" },
                  "stroke": { "expr": "lit", "value": "currentColor" },
                  "stroke-width": { "expr": "lit", "value": "2" },
                  "class": { "expr": "lit", "value": "h-4 w-4" }
                },
                "children": [
                  { "kind": "element", "tag": "polygon", "props": { "points": { "expr": "lit", "value": "5 3 19 12 5 21 5 3" } } }
                ]
              },
              { "kind": "text", "value": { "expr": "lit", "value": "Try in Playground" } }
            ]
          }
        ]
      },
      {
        "kind": "element",
        "tag": "section",
        "children": [
          {
            "kind": "element",
            "tag": "h2",
            "props": {
              "class": { "expr": "lit", "value": "mb-4 text-xl font-semibold text-foreground" }
            },
            "children": [
              { "kind": "text", "value": { "expr": "lit", "value": "Features Used" } }
            ]
          },
          {
            "kind": "element",
            "tag": "ul",
            "props": {
              "class": { "expr": "lit", "value": "flex flex-wrap gap-2" }
            },
            "children": [
              {
                "kind": "each",
                "items": { "expr": "get", "base": { "expr": "index", "base": { "expr": "import", "name": "examples", "path": "items" }, "key": { "expr": "route", "name": "slug" } }, "path": "featuresPreview" },
                "as": "feature",
                "body": {
                  "kind": "element",
                  "tag": "li",
                  "props": {
                    "class": {
                      "expr": "cond",
                      "if": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "feature", "path": "category" }, "right": { "expr": "lit", "value": "state" } },
                      "then": { "expr": "lit", "value": "rounded-full backdrop-blur-md bg-purple-400/10 border border-purple-400/20 px-3 py-1 text-xs font-medium text-purple-600 dark:text-purple-300 shadow-sm transition-all hover:bg-purple-400/20 hover:border-purple-400/30" },
                      "else": {
                        "expr": "cond",
                        "if": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "feature", "path": "category" }, "right": { "expr": "lit", "value": "action" } },
                        "then": { "expr": "lit", "value": "rounded-full backdrop-blur-md bg-cyan-400/10 border border-cyan-400/20 px-3 py-1 text-xs font-medium text-cyan-600 dark:text-cyan-300 shadow-sm transition-all hover:bg-cyan-400/20 hover:border-cyan-400/30" },
                        "else": {
                          "expr": "cond",
                          "if": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "feature", "path": "category" }, "right": { "expr": "lit", "value": "component" } },
                          "then": { "expr": "lit", "value": "rounded-full backdrop-blur-md bg-pink-400/10 border border-pink-400/20 px-3 py-1 text-xs font-medium text-pink-600 dark:text-pink-300 shadow-sm transition-all hover:bg-pink-400/20 hover:border-pink-400/30" },
                          "else": { "expr": "lit", "value": "rounded-full backdrop-blur-md bg-gray-400/10 dark:bg-white/5 border border-gray-400/20 dark:border-white/10 px-3 py-1 text-xs font-medium text-gray-600 dark:text-gray-300 shadow-sm transition-all hover:bg-gray-400/20 dark:hover:bg-white/10 hover:border-gray-400/30 dark:hover:border-white/20" }
                        }
                      }
                    }
                  },
                  "children": [
                    { "kind": "text", "value": { "expr": "var", "name": "feature", "path": "name" } }
                  ]
                }
              },
              {
                "kind": "each",
                "items": { "expr": "get", "base": { "expr": "index", "base": { "expr": "import", "name": "examples", "path": "items" }, "key": { "expr": "route", "name": "slug" } }, "path": "featuresExtra" },
                "as": "feature",
                "body": {
                  "kind": "element",
                  "tag": "li",
                  "props": {
                    "class": {
                      "expr": "cond",
                      "if": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "feature", "path": "category" }, "right": { "expr": "lit", "value": "state" } },
                      "then": { "expr": "lit", "value": "rounded-full backdrop-blur-md bg-purple-400/10 border border-purple-400/20 px-3 py-1 text-xs font-medium text-purple-600 dark:text-purple-300 shadow-sm transition-all hover:bg-purple-400/20 hover:border-purple-400/30" },
                      "else": {
                        "expr": "cond",
                        "if": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "feature", "path": "category" }, "right": { "expr": "lit", "value": "action" } },
                        "then": { "expr": "lit", "value": "rounded-full backdrop-blur-md bg-cyan-400/10 border border-cyan-400/20 px-3 py-1 text-xs font-medium text-cyan-600 dark:text-cyan-300 shadow-sm transition-all hover:bg-cyan-400/20 hover:border-cyan-400/30" },
                        "else": {
                          "expr": "cond",
                          "if": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "feature", "path": "category" }, "right": { "expr": "lit", "value": "component" } },
                          "then": { "expr": "lit", "value": "rounded-full backdrop-blur-md bg-pink-400/10 border border-pink-400/20 px-3 py-1 text-xs font-medium text-pink-600 dark:text-pink-300 shadow-sm transition-all hover:bg-pink-400/20 hover:border-pink-400/30" },
                          "else": { "expr": "lit", "value": "rounded-full backdrop-blur-md bg-gray-400/10 dark:bg-white/5 border border-gray-400/20 dark:border-white/10 px-3 py-1 text-xs font-medium text-gray-600 dark:text-gray-300 shadow-sm transition-all hover:bg-gray-400/20 dark:hover:bg-white/10 hover:border-gray-400/30 dark:hover:border-white/20" }
                        }
                      }
                    }
                  },
                  "children": [
                    { "kind": "text", "value": { "expr": "var", "name": "feature", "path": "name" } }
                  ]
                }
              }
            ]
          }
        ]
      },
      {
        "kind": "element",
        "tag": "section",
        "children": [
          {
            "kind": "element",
            "tag": "h2",
            "props": {
              "class": { "expr": "lit", "value": "mb-4 text-xl font-semibold text-foreground" }
            },
            "children": [
              { "kind": "text", "value": { "expr": "lit", "value": "How to Run" } }
            ]
          },
          {
            "kind": "element",
            "tag": "div",
            "props": {
              "class": { "expr": "lit", "value": "space-y-3" }
            },
            "children": [
              {
                "kind": "each",
                "items": { "expr": "get", "base": { "expr": "index", "base": { "expr": "import", "name": "examples", "path": "items" }, "key": { "expr": "route", "name": "slug" } }, "path": "runCommands" },
                "as": "cmd",
                "body": {
                  "kind": "element",
                  "tag": "div",
                  "props": {
                    "class": { "expr": "lit", "value": "group relative" }
                  },
                  "children": [
                    {
                      "kind": "element",
                      "tag": "pre",
                      "props": {
                        "class": { "expr": "lit", "value": "overflow-x-auto rounded-lg border border-border bg-muted p-4 text-sm" }
                      },
                      "children": [
                        {
                          "kind": "element",
                          "tag": "code",
                          "props": {
                            "class": { "expr": "lit", "value": "font-mono text-foreground" }
                          },
                          "children": [
                            { "kind": "text", "value": { "expr": "var", "name": "cmd" } }
                          ]
                        }
                      ]
                    }
                  ]
                }
              }
            ]
          }
        ]
      },
      {
        "kind": "element",
        "tag": "section",
        "children": [
          {
            "kind": "element",
            "tag": "h2",
            "props": {
              "class": { "expr": "lit", "value": "mb-4 text-xl font-semibold text-foreground" }
            },
            "children": [
              { "kind": "text", "value": { "expr": "lit", "value": "Source Code" } }
            ]
          },
          {
            "kind": "code",
            "content": { "expr": "index", "base": { "expr": "import", "name": "examples", "path": "codeStrings" }, "key": { "expr": "route", "name": "slug" } },
            "language": { "expr": "lit", "value": "json" }
          }
        ]
      }
    ]
  }
}
