                            { "kind": "element", "tag": "input", "props": { "type": { "expr": "lit", "value": "radio" }, "checked": { "expr": "lit", "value": true }, "class": { "expr": "lit", "value": "h-4 w-4 rounded-full border-input" } } },
                            { "kind": "element", "tag": "span", "props": { "class": { "expr": "lit", "value": "text-sm" } }, "children": [{ "kind": "text", "value": { "expr": "lit", "value": "Radio option" } }] }
                          ]
