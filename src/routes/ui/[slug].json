                          "tag": "label",
                          "props": { "class": { "expr": "lit", "value": "flex items-center gap-2" } },
                          "children": [
                            { "kind": "element", "tag": "input", "props": { "type": { "expr": "lit", "value": "checkbox" }, "checked": { "expr": "lit", "value": true }, "class": { "expr": "lit", "value": "h-4 w-4 rounded border-input" } } },
                            { "kind": "element", "tag": "span", "props": { "class": { "expr": "lit", "value": "text-sm" } }, "children": [{ "kind": "text", "value": { "expr": "lit", "value": "Checkbox" } }] }
