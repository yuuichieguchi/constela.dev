                        "then": {
                          "kind": "element",
                          "tag": "select",
                          "props": { "class": { "expr": "lit", "value": "flex h-10 w-full max-w-xs rounded-md border border-input bg-background px-3 py-2 text-sm" } },
                          "children": [
