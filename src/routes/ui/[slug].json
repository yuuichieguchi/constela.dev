                          "as": "size",
                          "body": {
                            "kind": "element",
                            "tag": "span",
                            "props": {
                              "class": { "expr": "lit", "value": "rounded-full bg-secondary/50 border border-secondary px-3 py-1 text-sm text-secondary-foreground" }
                            },
                            "children": [
                              { "kind": "text", "value": { "expr": "var", "name": "size" } }
                            ]
                          }
                        }
