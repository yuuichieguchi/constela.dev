                      },
                      {
                        "kind": "if",
                        "condition": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "comp", "path": "slug" }, "right": { "expr": "lit", "value": "textarea" } },
                        "then": {
                          "kind": "element",
                          "tag": "textarea",
                          "props": {
