                    {
                      "kind": "element",
                      "tag": "div",
                      "props": {
                        "class": { "expr": "lit", "value": "flex flex-wrap gap-2" }
                      },
                      "children": [
                        {
                          "kind": "each",
                          "items": { "expr": "var", "name": "comp", "path": "accessibility" },
                          "as": "a11y",
                          "body": {
                            "kind": "element",
                            "tag": "code",
                            "props": {
                              "class": { "expr": "lit", "value": "rounded bg-green-400/10 border border-green-400/20 px-2 py-1 text-sm text-green-600 dark:text-green-300" }
