                  {
                    "kind": "element",
                    "tag": "div",
                    "props": { "class": { "expr": "lit", "value": "rounded-lg border bg-card p-8 flex items-center justify-center" } },
                    "children": [
                      {
                        "kind": "if",
                        "condition": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "comp", "path": "slug" }, "right": { "expr": "lit", "value": "button" } },
                        "then": {
