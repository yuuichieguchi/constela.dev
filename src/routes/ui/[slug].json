{
  "version": "1.0",
  "route": {
    "path": "/ui/:slug",
    "layout": "ui",
    "layoutParams": {
      "currentSlug": { "expr": "route", "name": "slug", "source": "param" }
    },
    "meta": {
      "title": { "expr": "concat", "items": [{ "expr": "data", "name": "component", "path": "name" }, { "expr": "lit", "value": " - UI Components - Constela" }] },
      "description": { "expr": "data", "name": "component", "path": "description" }
    },
    "getStaticPaths": {
      "source": "uiData",
      "params": {
        "slug": { "expr": "var", "name": "item", "path": "slug" }
      }
    }
  },
  "imports": {
    "uiData": "../../data/ui-components.json"
  },
  "data": {
    "component": {
      "type": "file",
      "path": "../../data/ui-components.json"
    }
  },
  "view": {
    "kind": "element",
    "tag": "div",
    "props": {
      "class": { "expr": "lit", "value": "mx-auto max-w-4xl px-6 py-12 space-y-8" }
    },
    "children": [
      {
        "kind": "element",
        "tag": "a",
        "props": {
          "href": { "expr": "lit", "value": "/ui" },
          "class": { "expr": "lit", "value": "inline-flex items-center text-sm text-muted-foreground hover:text-foreground" }
        },
        "children": [
          { "kind": "text", "value": { "expr": "lit", "value": "‚Üê Back to Components" } }
        ]
      },
      {
        "kind": "each",
        "items": { "expr": "import", "name": "uiData", "path": "components" },
        "as": "comp",
        "body": {
          "kind": "if",
          "condition": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "comp", "path": "slug" }, "right": { "expr": "route", "name": "slug", "source": "param" } },
          "then": {
            "kind": "element",
            "tag": "div",
            "props": {
              "class": { "expr": "lit", "value": "space-y-8" }
            },
            "children": [
              {
                "kind": "element",
                "tag": "header",
                "children": [
                  {
                    "kind": "element",
                    "tag": "div",
                    "props": {
                      "class": { "expr": "lit", "value": "flex items-center gap-3" }
                    },
                    "children": [
                      {
                        "kind": "element",
                        "tag": "h1",
                        "props": {
                          "class": { "expr": "lit", "value": "text-3xl font-bold text-foreground" }
                        },
                        "children": [
                          { "kind": "text", "value": { "expr": "var", "name": "comp", "path": "name" } }
                        ]
                      },
                      {
                        "kind": "element",
                        "tag": "span",
                        "props": {
                          "class": {
                            "expr": "cond",
                            "if": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "comp", "path": "category" }, "right": { "expr": "lit", "value": "basic" } },
                            "then": { "expr": "lit", "value": "rounded-full bg-blue-400/10 border border-blue-400/20 px-3 py-1 text-sm text-blue-600 dark:text-blue-300" },
                            "else": {
                              "expr": "cond",
                              "if": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "comp", "path": "category" }, "right": { "expr": "lit", "value": "feedback" } },
                              "then": { "expr": "lit", "value": "rounded-full bg-green-400/10 border border-green-400/20 px-3 py-1 text-sm text-green-600 dark:text-green-300" },
                              "else": {
                                "expr": "cond",
                                "if": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "comp", "path": "category" }, "right": { "expr": "lit", "value": "overlay" } },
                                "then": { "expr": "lit", "value": "rounded-full bg-purple-400/10 border border-purple-400/20 px-3 py-1 text-sm text-purple-600 dark:text-purple-300" },
                                "else": {
                                  "expr": "cond",
                                  "if": { "expr": "bin", "op": "==", "left": { "expr": "var", "name": "comp", "path": "category" }, "right": { "expr": "lit", "value": "navigation" } },
                                  "then": { "expr": "lit", "value": "rounded-full bg-orange-400/10 border border-orange-400/20 px-3 py-1 text-sm text-orange-600 dark:text-orange-300" },
                                  "else": { "expr": "lit", "value": "rounded-full bg-cyan-400/10 border border-cyan-400/20 px-3 py-1 text-sm text-cyan-600 dark:text-cyan-300" }
                                }
                              }
                            }
                          }
                        },
                        "children": [
                          { "kind": "text", "value": { "expr": "var", "name": "comp", "path": "category" } }
                        ]
                      }
                    ]
                  },
                  {
                    "kind": "element",
                    "tag": "p",
                    "props": {
                      "class": { "expr": "lit", "value": "mt-2 text-lg text-muted-foreground" }
                    },
                    "children": [
                      { "kind": "text", "value": { "expr": "var", "name": "comp", "path": "description" } }
                    ]
                  }
                ]
              },
              {
                "kind": "element",
                "tag": "section",
                "props": {
                  "class": { "expr": "lit", "value": "space-y-4" }
                },
                "children": [
                  {
                    "kind": "element",
                    "tag": "h2",
                    "props": {
                      "class": { "expr": "lit", "value": "text-xl font-semibold text-foreground" }
                    },
                    "children": [
                      { "kind": "text", "value": { "expr": "lit", "value": "Parameters" } }
                    ]
                  },
                  {
                    "kind": "element",
                    "tag": "div",
                    "props": {
                      "class": { "expr": "lit", "value": "flex flex-wrap gap-2" }
                    },
                    "children": [
                      {
                        "kind": "each",
                        "items": { "expr": "var", "name": "comp", "path": "params" },
                        "as": "param",
                        "body": {
                          "kind": "element",
                          "tag": "code",
                          "props": {
                            "class": { "expr": "lit", "value": "rounded bg-muted px-2 py-1 text-sm text-foreground" }
                          },
                          "children": [
                            { "kind": "text", "value": { "expr": "var", "name": "param" } }
                          ]
                        }
                      }
                    ]
                  }
                ]
              },
              {
                "kind": "if",
                "condition": { "expr": "var", "name": "comp", "path": "variants" },
                "then": {
                  "kind": "element",
                  "tag": "section",
                  "props": {
                    "class": { "expr": "lit", "value": "space-y-4" }
                  },
                  "children": [
                    {
                      "kind": "element",
                      "tag": "h2",
                      "props": {
                        "class": { "expr": "lit", "value": "text-xl font-semibold text-foreground" }
                      },
                      "children": [
                        { "kind": "text", "value": { "expr": "lit", "value": "Variants" } }
                      ]
                    },
                    {
                      "kind": "element",
                      "tag": "div",
                      "props": {
                        "class": { "expr": "lit", "value": "flex flex-wrap gap-2" }
                      },
                      "children": [
                        {
                          "kind": "each",
                          "items": { "expr": "var", "name": "comp", "path": "variants" },
                          "as": "variant",
                          "body": {
                            "kind": "element",
                            "tag": "span",
                            "props": {
                              "class": { "expr": "lit", "value": "rounded-full bg-primary/10 border border-primary/20 px-3 py-1 text-sm text-primary" }
                            },
                            "children": [
                              { "kind": "text", "value": { "expr": "var", "name": "variant" } }
                            ]
                          }
                        }
                      ]
                    }
                  ]
                }
              },
              {
                "kind": "if",
                "condition": { "expr": "var", "name": "comp", "path": "sizes" },
